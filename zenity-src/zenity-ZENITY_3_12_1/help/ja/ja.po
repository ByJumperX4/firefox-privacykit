"\t;;\n"
"    -1)\n"
"        echo \"An unexpected error has occurred.\"\n"
"\t;;\n"
"esac\n"
msgstr ""
"\n"
"#!/bin/sh\n"
"\n"
"zenity --forms --title=\"友達の追加\" \\\n"
