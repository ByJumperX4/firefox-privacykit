#, no-wrap
msgid ""
"\n"
"#!/bin/sh\n"
"\n"
"zenity --forms --title=\"Add Friend\" \\\n"
"\t--text=\"Enter information about your friend.\" \\\n"
"\t--separator=\",\" \\\n"
"\t--add-entry=\"First Name\" \\\n"
"\t--add-entry=\"Family Name\" \\\n"
