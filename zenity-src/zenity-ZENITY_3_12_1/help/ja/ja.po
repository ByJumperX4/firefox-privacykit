"\t--separator=\",\" \\\n"
"\t--add-entry=\"名\" \\\n"
"\t--add-entry=\"姓\" \\\n"
"\t--add-entry=\"電子メール\" \\\n"
"\t--add-calendar=\"誕生日\" &gt;&gt; addr.csv\n"
"\n"
"case $? in\n"
"    0)\n"
"        echo \"友達が追加されました。\";;\n"
"    1)\n"
"        echo \"友達は追加されませんでした。\"\n"
"\t;;\n"
"    -1)\n"
"        echo \"予期せぬエラーが発生しました。\"\n"
"\t;;\n"
"esac\n"

