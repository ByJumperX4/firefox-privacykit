#, no-wrap
msgid ""
"\n"
"#!/bin/bash\n"
"\n"
"zenity --warning \\\n"
"--text=\"Disconnect the power cable to avoid electrical shock.\"\n"
msgstr ""
"\n"
"#!/bin/bash\n"
"\n"
"zenity --warning \\\n"
"--text=\"Desconecte o cable eléctrico para evitar un shock eléctrico.\"\n"

